[alias]
	plog = log --pretty=format:'%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
	patch = diff --no-color
	unstage = reset HEAD
	staged = diff --staged
	all = add -A
	amend = commit --amend -C HEAD
	credit = commit --amend -C HEAD --author
	undo = reset --soft HEAD^
[core]
	excludesfile = /home/bshah/.gitignorerc
[commit]
	template = /home/bshah/commit-template
